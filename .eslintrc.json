{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "one-var": "off",
        "array-callback-return": "off",
        "one-var-declaration-per-line": "off",
        "new-cap": "off",
        "consistent-return":"off",
        "no-param-reassign": "off", 
        "comma-dangle": "off",
        "default-case":"off",
        "no-console":"warn",
        "indent": ["error", 2],
        "no-unreachable":"error",
        "curly": ["off"],
        "no-shadow":"off"
    }
}


